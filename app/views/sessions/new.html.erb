<header class="page-header">
  <h1>Login</h1>
  </header>



<%= form_tag '/login' do %>
  <div class="form-group mt-3">
    Email: <%= text_field_tag :email %>
  </div>

  <div class="form-group mt-3">
    Password: <%= password_field_tag :password %>
  </div>

  <div class="mt-3">
  <%= submit_tag "Submit" %>
  </div>

<% end %>